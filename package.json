{
  "name": "integrator-browserstack-tunnel",
  "version": "1.0.0",
  "description": "Start BrowserStackLocal secure tunnel for running tests against local environments",
  "main": "out/index.js",
  "files": ["bin/BrowserStackLocal"],
  "scripts": {
    "build": "babel --optional runtime src --out-dir out",
    "watch": "babel --watch --optional runtime src --out-dir out",
    "test": "npm run lint && npm run specs",
    "specs": "npm run build && ava out/index.spec.js",
    "lint": "eslint src/*.js -c .eslintrc",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phuu/integrator-browserstack-tunnel.git"
  },
  "author": "Twitter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/phuu/integrator-browserstack-tunnel/issues"
  },
  "homepage": "https://github.com/phuu/integrator-browserstack-tunnel#readme",
  "dependencies": {
    "babel": "^5.8.23",
    "babel-runtime": "^5.8.20"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "eslint": "^1.6.0",
    "ava": "^0.6.0",
    "mock-spawn": "^0.2.6"
  }
}
